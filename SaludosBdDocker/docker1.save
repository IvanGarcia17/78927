from ubuntu
workdir /tareasmatadoras
expose 880
cmd ["/tareasmatadoras/mvnw","spring-boot:run"]
run apt-get update
run apt-get install -y openjdk-8-jdk
run apt-get install -y curl
run apt-get install -y unzip
run curl-G hhtps://start.spring.io/starter.zip -d dependencies=web-services -d javaVersion=8 -o demo.zip

run unzip demo.zip
